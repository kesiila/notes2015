   <script>
 var snExposeSign = "12_";
 //SN节点曝光串拼接
 var exposureExpStr = "";
 if(typeof snExpose_SPFeatId_Str != "undefined"){
 exposureExpStr += "(spfeat:"+snExpose_SPFeatId_Str + ")";
 }
 exposureExpStr += '(feat:'
 + (typeof snExpose_hotId_Str != 'undefined' ? snExpose_hotId_Str : '')
 + (typeof snExpose_featId_Str != 'undefined' ? snExpose_featId_Str : '')
 + (typeof snExpose_price_Str != 'undefined' ? snExpose_price_Str : '')
 + ')';

 if(typeof snExpose_catId_Str != "undefined"){
 exposureExpStr += "(cat:"+snExpose_catId_Str + ")";
 }
 if(typeof snExpose_layercCatId_Str != "undefined"){
 exposureExpStr += "(layerCat:"+snExpose_layercCatId_Str + ")";
 }
 //区域曝光打点
 iSearchPV.iSubject.push("sn_show_" + snExposeSign + encodeURI(exposureExpStr));
 </script>

